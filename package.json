{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --tunnel",
    "start-web": "expo start --web --tunnel",
    "start-web-dev": "DEBUG=expo* expo start --web --tunnel",
    "start:dev": "APP_ENV=development expo start",
    "start:staging": "APP_ENV=staging expo start",
    "start:prod": "APP_ENV=production expo start",
    "web:dev": "APP_ENV=development expo start --web",
    "web:staging": "APP_ENV=staging expo start --web",
    "web:prod": "APP_ENV=production expo start --web",
    "build:web": "APP_ENV=production expo export:web",
    "lint": "eslint .",
    "typescript": "tsc --noEmit",
    "prepare": "husky",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "storybook:start": "EXPO_PUBLIC_USE_STORYBOOK=true expo start",
    "storybook:ios": "EXPO_PUBLIC_USE_STORYBOOK=true expo start --ios",
    "storybook:android": "EXPO_PUBLIC_USE_STORYBOOK=true expo start --android",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.0",
    "@sentry/react-native": "^6.10.0",
    "@supabase/supabase-js": "^2.38.5",
    "@tanstack/react-query": "^5.35.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "dotenv": "^16.3.1",
    "expo": "~52.0.41",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.0",
    "expo-image": "~2.0.7",
    "expo-image-picker": "^16.0.6",
    "expo-linear-gradient": "^14.0.1",
    "expo-linking": "~7.0.3",
    "expo-location": "^18.0.8",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-performance": "^5.1.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-react-native-web": "^8.6.11",
    "@storybook/blocks": "^8.6.11",
    "@storybook/react-native": "^8.6.11",
    "@types/node": "^22.13.14",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "storybook": "^8.6.11",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
