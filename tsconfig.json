{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"],
        "@app/*": ["app/*"],
        "@components/*": ["components/*"],
        "@constants/*": ["constants/*"],
        "@services/*": ["services/*"],
        "@store/*": ["store/*"],
        "@types/*": ["types/*"],
        "@utils/*": ["utils/*"],
        "@assets/*": ["assets/*"]
      }
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      ".expo/types/**/*.ts",
      "expo-env.d.ts"
    ]
  }
  